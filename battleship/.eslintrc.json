{
    "extends": "airbnb",
    "rules": {
        "no-console":"warn",
        "no-unused-vars": "warn",
        "linebreak-style":"off",
        "prefer-const":"warn",
        "implicit-arrow-linebreak":"off",
        "no-undef": "warn",
        "comma-dangle":"off",
        "function-paren-newline":"off"
    }
}
